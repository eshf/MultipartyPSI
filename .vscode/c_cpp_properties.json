{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": ["VERSION=1"],
            "intelliSenseMode": "linux-gcc-x64",
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "configurationProvider": "ms-vscode.cpptools",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "mergeConfigurations": true
        }
    ],
    "version": 4
}